<?xml version ="1.0"?>
<robot name ="2r_planar">
    <link name ="fixed">
        <visual>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
        </visual>
    </link>
    <link name="link_1">
        <visual>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder length="1" radius="0.1"/>
            </geometry>
        </visual>
        <inertial>
            <mass value="12.456"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="1.042"/>
        </inertial>
    </link>
    <link name="link_2">
        <visual>
            <origin xyz="0 0 0"/>
            <geometry>
                <cylinder length="1" radius="0.1"/>
            </geometry>
        </visual>
        <inertial>
            <mass value="12.456"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="1.042"/>
        </inertial>
    </link>

    <joint name="joint_1" type="revolute">
        <axis xyz="0 0 1"/>
        <origin rpy="0 0 0"  xyz="0 0 0.05"/>
        <limit effort="1000.0" lower="0.0" upper="3.14" velocity="0.5"/>
        <parent link="fixed"/>
        <child link="link_1"/>
    </joint> 

    <joint name="joint_2" type="revolute">
        <axis xyz="0 -1 0"/>
        <origin rpy="0 0 0"  xyz="0 0 0"/>
        <limit effort="1000.0" lower="0.0" upper="3.14" velocity="0.5"/>
        <parent link="link_1"/>
        <child link="link_2"/>
    </joint>   
</robot>
